{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "generate-ids": {
      "description": "A blank schematic.",
      "factory": "./generate-ids/index#projectName",
      "schema": "./generate-ids/schema.json"
    }
  },
  "properties": {
    "prefix": {
      "type": "string",
